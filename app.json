{
  "expo": {
    "name": "kodomo-gallery",
    "slug": "garally",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "scheme": "kodomogallery",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ryohatada.kodomogallery",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "さくひんのほぞんのためにふぉとライブラリへのあくせすがひつようです。"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "aaee262b-2b46-4734-ace1-906dda41ff82"
      }
    }
  }
}
